generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/prismamongoclient"
}

datasource readdb {
  provider = "mongodb"
  url      = env("MONGODB_DATABASE_URL")
}

model Course {
  id          String    @id @default(auto()) @map("_id") @readdb.ObjectId
  course_id   String
  slug        String
  title       String
  description String?
  author_id   String
  contents    String?
}
