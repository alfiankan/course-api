generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_DATABASE_URL")
}

model Course {
  id          String  @id @default(uuid()) @db.VarChar(36)
  slug        String  @unique
  title       String  @db.VarChar(200)
  description String? @db.Text
  author_id   String  @db.VarChar(36)
  contents    String? @db.Text
}
